{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFramework": "Warning"
    },
    "File": {
      "Path": "logs/log.txt",
      "FileSizeLimitBytes": 10485760,
      "RetainedFileCountLimit": 5
    }
  },
  "AllowedHosts": "*"
}
